#include "../push_swap.h"

void	sb(Pile b)
{
	int temp;

	if (!a.tab[0] || !a.tab[1])
		return ;
	temp = a.tab[0];
	a.tab[0] = a.tab[1];
	a.tab[1] = temp;
	return ;
}
}
